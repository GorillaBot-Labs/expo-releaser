{
  "cli": {
    "version": ">= 0.42.4"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_ENV": "development"
      }
    },
    "staging": {
      "releaseChannel": "staging",
      "env": {
        "EXPO_ENV": "staging"
      }
    },
    "production": {
      "releaseChannel": "production",
      "env": {
        "EXPO_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
